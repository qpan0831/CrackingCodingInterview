    //return Kth element to the end of the list;
    public Node returnKth(Node head,int kth)
    {
        Node p1=head;
        Node p2=head;
        for(int i=0;i<kth;i++)
        {
            if(p2==null)
                return null;
            p2=p2.next;
        }
        while(p2.next!=null)
        {
            p1=p1.next;
            p2=p2.next;
        }
        return p1;
    }
